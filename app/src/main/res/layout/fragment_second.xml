<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/second_fragment11"
    tools:context=".SecondFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constn1"
        android:layout_width="match_parent"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/instagram"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/white"
            android:orientation="horizontal"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="  𝓘𝓷𝓼𝓽𝓪𝓰𝓻𝓪𝓶"
                android:textSize="25dp"
                android:textColor="@color/black"
                android:gravity="center"
                app:layout_constraintLeft_toLeftOf="parent"
                />
            <ImageView
                android:id="@+id/masseg"
                android:layout_width="25sp"
                android:layout_margin="10dp"
                android:layout_height="25sp"
                app:layout_constraintRight_toRightOf="parent"
                android:background="@drawable/messag"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginRight="10dp"/>


            <ImageView
                android:layout_width="25sp"
                android:layout_height="25sp"
                android:layout_margin="10dp"
                app:layout_constraintRight_toLeftOf="@id/masseg"
                android:background="@drawable/pluus"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginRight="15dp"/>


        </androidx.constraintlayout.widget.ConstraintLayout>



    <LinearLayout
        android:id="@+id/Storys"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2-Fragment"
        android:textSize="30dp"
        tools:layout_editor_absoluteY="0dp"
        app:layout_constraintTop_toBottomOf="@+id/instagram"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        >


        <androidx.cardview.widget.CardView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="1dp"
            app:cardCornerRadius="100dp"
            app:cardMaxElevation="20dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/rasmmm" />

        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="1dp"
            app:cardCornerRadius="100dp"
            app:cardMaxElevation="20dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/img_2" />

        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="1dp"
            app:cardCornerRadius="100dp"
            app:cardMaxElevation="20dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/img_3" />

        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="1dp"
            app:cardCornerRadius="100dp"
            app:cardMaxElevation="20dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/img_1" />

        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"

            android:layout_height="80dp"
            android:layout_margin="1dp"

            app:cardCornerRadius="100dp"
            app:cardMaxElevation="20dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/img_4" />

        </androidx.cardview.widget.CardView>


    </LinearLayout>
        <TextView
            android:id="@+id/name"
            android:layout_width="match_parent"
            android:layout_height="25sp"
            android:background="@color/white"
            android:textSize="12dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:text="  Your Story        xabibulloh_17     h_fayzullo_        kingryan     sardor____7"
            app:layout_constraintTop_toBottomOf="@id/Storys"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/fotonastroki"
        android:layout_width="match_parent"
        android:layout_height="50sp"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/constn1"
        app:layout_constraintLeft_toLeftOf="parent">


        <androidx.cardview.widget.CardView
            android:id="@+id/cardd1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:cardElevation="100dp"
            app:cardCornerRadius="100dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            >

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/rasmmm"
                android:elevation="100dp"/>


        </androidx.cardview.widget.CardView>


        <TextView
            android:layout_width="80dp"
            android:layout_height="40sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/cardd1"
            android:text="  jams1__d\n  Ц1"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/nuqta"/>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/photo"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/fotonastroki"
        >


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:background="@drawable/img_6"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/photo">

        <ImageView
            android:id="@+id/like"
            android:layout_width="25sp"
            android:layout_height="25sp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/like"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />

        <ImageView
            android:id="@+id/comment"
            android:layout_width="25sp"
            android:layout_height="25sp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/comment"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@id/like" />


        <ImageView
            android:id="@+id/jonatish"
            android:layout_width="25sp"
            android:layout_height="25sp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="10dp"
            android:background="@drawable/sent"
            app:layout_constraintLeft_toRightOf="@id/comment" />

        <ImageView
            android:id="@+id/savee"
            android:layout_width="25dp"
            android:layout_height="match_parent"
            android:layout_marginRight="10dp"
            android:background="@drawable/savee"
            app:layout_constraintRight_toRightOf="parent" />

        <ImageView
            android:layout_width="25dp"
            android:layout_height="match_parent"
            android:layout_marginRight="10dp"
            android:background="@drawable/saxrant"
            app:layout_constraintRight_toLeftOf="@id/savee" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:text="  1,150 likes"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="   jamsh1__d   Я👑"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/black"

        android:text="   Sheyx_271 ✊🏻✌🏻\n   sheyx_271 🔥🔥🔥🔥 "
        app:layout_constraintTop_toBottomOf="@+id/textView3" />



</androidx.constraintlayout.widget.ConstraintLayout>